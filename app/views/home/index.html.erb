<%= form_tag("/", method: "get") do %>
  <%= label_tag(:q_artist_name, "Search Artist:") %>
  <%= text_field_tag(:q_artist_name) %>
  <%= submit_tag("Go!") %>
<% end %>

<%= form_tag("/", method: "get") do %>
  <%= label_tag(:q_venue_name, "Search Venue by Name:") %>
  <%= text_field_tag(:q_artist_name) %>
  <%= submit_tag("Go!") %>
<% end %>

<%= form_tag("/", method: "get") do %>
  <%= label_tag(:q_venue_zipCode, "Search Venue by Zip Code:") %>
  <%= text_field_tag(:q_artist_zipCode) %>
  <%= submit_tag("Go!") %>
<% end %>

<% if @artist_name_results.present? %>

ARTIST: <%= params[:q_artist_name] %>
<br>
NAMES: 
  <ol>  
    <% @artist_name_results.each do |n| %>
      <li><%= n %></li>
    <% end %>
  </ol>

<% end %>

<% if user_signed_in? %>

<ul>

<li>Email: <%= @user.email %></li>
<li>Name: <%= @user.name %></li>
<li>Image: <%= image_tag @user.image %></li>

</ul>

<% end %>
