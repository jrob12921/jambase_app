<h2>My Events</h2>

<ol>
  <% @jb_events.each_with_index do |j, i| %>
    <li>
      <ul>   
          <li>Artists:
            <ol>
              <% j[:event_artists].each do |a| %>
                <li><%= a['Name'] %></li>
              <% end %>
            </ol>
          </li>
          <li>Venue: <%= j[:event_venue]['Name'] %></li>
          <li>Date: <%= DateTime.parse(j[:event_date]).strftime("%-m/%-d/%y") %></li>
          <li><%= link_to("Group Chat", event_group_messages_path(@user_events[i].event_id), method: :get) %></li>
      </ul>
    </li>
    <br>
  <% end %>
</ol>